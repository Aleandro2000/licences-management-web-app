{
    "env": {
        "es2021": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "no-redeclare": [0, {"builtinGlobals": false}],
        "react/prop-types": "off",
        "globals": {
            "_": "readonly"
        }
    }
}
